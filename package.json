{
  "name": "react-proptypes-url-validator",
  "version": "0.0.1",
  "description": "custom url validator for React PropTypes",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/irvinebroque/react-proptypes-url-validator.git"
  },
  "keywords": [
    "react",
    "propTypes",
    "url-regex",
    "validator"
  ],
  "author": "Brendan Irvine-Broque <brendanib@gmail.com> (http://irvinebroque.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/irvinebroque/react-proptypes-url-validator/issues"
  },
  "homepage": "https://github.com/irvinebroque/react-proptypes-url-validator",
  "dependencies": {
    "url-regex": "^3.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"],
    "unmockedModulePathPatterns": ["react", "<rootDir>/node_modules/url-regex/node_modules"]
  },
  "eslint": {
    "env": {
      "es6": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "templateStrings": true
    }
  },
  "devDependencies": {
    "babel-jest": "^5.0.1",
    "jest-cli": "^0.4.0",
    "react": "^0.13.2",
    "react-tools": "^0.13.2"
  }
}
